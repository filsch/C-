void fileHandler(string filename){
/*chdir()*/
	const int bufferSize = MAX_PATH;

	ifstream fileContent;
	fileContent.open(filename);

	cout <<  << endl;
	if (fileContent.fail()){
		cout << "Ã…pning av fil feilet" << endl;
		exit(1);
	}

	string linje;

	while (getline(fileContent, linje)) {
		cout << linje << endl;
    }
}


